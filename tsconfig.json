{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "ESNext",
    "target": "ES6",
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": false,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noUncheckedIndexedAccess": false
  }
}